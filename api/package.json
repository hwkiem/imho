{
    "name": "api",
    "version": "1.0.0",
    "description": "The initial davis diver project.",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist/ node_modules/",
        "build": "yarn clean && tsc",
        "watch": "tsc -w",
        "start": "nodemon dist/index.js",
        "migrate:initial": "yarn mikro-orm migration:create --initial",
        "migrate:up": "yarn mikro-orm migration:up",
        "migrate:create": "yarn mikro-orm migration:create",
        "migrate:down": "yarn mikro-orm migration:down",
        "migrate:list": "yarn mikro-orm migration:list",
        "migrate:pending": "yarn mikro-orm migration:pending"
    },
    "dependencies": {
        "@googlemaps/google-maps-services-js": "^3.1.16",
        "@mikro-orm/cli": "^4.5.10",
        "@mikro-orm/core": "^4.5.10",
        "@mikro-orm/postgresql": "^4.5.10",
        "apollo-server": "^3.1.2",
        "argon2": "^0.28.2",
        "class-validator": "^0.13.1",
        "connect-redis": "^6.0.0",
        "cors": "^2.8.5",
        "dotenv-cli": "^4.0.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "graphql": "^15.5.1",
        "graphql-fields-to-relations": "^1.0.5",
        "ioredis": "^4.27.7",
        "knex": "^0.95.11",
        "knex-postgis": "^0.14.1",
        "pg": "^8.7.1",
        "pg-range": "^1.1.0",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1",
        "typedi": "^0.10.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/connect-pg-simple": "^4.2.4",
        "@types/connect-redis": "^0.0.17",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/express-session": "^1.17.4",
        "@types/ioredis": "^4.26.7",
        "@types/node": "^16.4.14",
        "@types/pg": "^8.6.1",
        "@types/pg-pool": "^2.0.3",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.2.0",
        "@typescript-eslint/parser": "^5.2.0",
        "eslint": "^8.1.0",
        "eslint-config-prettier": "^8.3.0",
        "nodemon": "^2.0.12",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "ts-node": "^10.4.0",
        "typescript": "^4.3.5"
    },
    "mikro-orm": {
        "useTsNode": true,
        "configPaths": [
            "./src/mikro-orm.config.ts",
            "./dist/mikro-orm.config.js"
        ]
    }
}
